<% provide :title, t("report.title") %>
<div class="form-search">
  <%= form_tag reports_path, method: :get do %>
    <%= text_field_tag :search, params[:search], required: true,
      placeholder: t("report.search") %>
    <%= submit_tag t("search.search") %>
  <% end %>
</div>
<%= render "shared/result_report" %>
